{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "ServiceStateEnumeration.schema.json",
    "title": "ServiceStateEnumeration",
    "definitions": {
        "ServiceStateEnumeration": {
            "$id": "#ServiceStateEnumeration",
            "type": "string",
            "description": "The life cycle state of the service",
